.date-background {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
}

.date-gray-background {
    position: absolute;
    width: 100%;
    top: 0;
    background: rgba(0, 0, 0, .5);
    height: calc(100% - 816rpx);
    z-index: 999;
}

.date-container {
    position: absolute;
    width: 100%;
    height: 816rpx;
    overflow: hidden;
    background: #fff;
    bottom: 0;
    z-index: 999;
    border-radius: 24rpx 24rpx 0 0;
}

.date-confirm {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 10rpx 32rpx;
    font-size: 34rpx;
    border-bottom: 1rpx solid #E7E8EA;
    height: 80rpx;
}

.indicator {
    height: 100rpx;
    border: 1rpx solid #E5E8E8;
}

.confirm {
    font-family: PingFangSC-Regular;
    font-size: 32rpx;
    color: #14AA82;
    letter-spacing: 0;
    font-weight: 400;
}

.cancel {
    font-family: PingFangSC-Regular;
    font-size: 32rpx;
    color: #909399;
    letter-spacing: 0;
    font-weight: 400;
}

.search {
    width: 666rpx;
    height: 64rpx;
    background: #F0F7F7;
    border-radius: 32rpx;
    margin: 20rpx auto 0;
    padding: 0 10rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.search input {
    flex: 1;
    height: inherit;
    background: #F0F7F7;
    border-radius: 32rpx;
    padding: 0 20rpx;
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #23364E;
    letter-spacing: 0;
    line-height: 28rpx;
    font-weight: 400;
}

.imgs {
    width: 30rpx;
}

.imgs image {
    width: 100%;
}

.picker_show {
    width: 100%;
    height: 76%;
    position: absolute;
    bottom: 0rpx;
    text-align: center;
    background: #fff
}

.select_item {
    height: 60rpx;
    line-height: 60rpx
}

.wxs1 {
    line-height: 100rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.active {
    color: #14AA82 !important;
}