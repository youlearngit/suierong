.sawtooth-bor {
  display: flex;
  height:25rpx;
  color: #333;
  box-sizing: border-box;
  background: radial-gradient(transparent 0, transparent 8rpx, #fff 8rpx);
  background-size: 30rpx 28rpx;
  background-position: -1rpx 8rpx;
  background-color: #f2f2f2;
}
