@import '/assets/styles/weui.wxss';
/* page{background:#eeeeee;} */
page{background:#ffffff;height: auto}
.head{position:relative;width:100%;height:368rpx;}
.headBg{position:absolute;top:0;left:0;width:100%;height:100%;z-index:-1;}
.setInfo{position:absolute;right:0;top:130rpx;width:210rpx;height:64rpx;}
.setInfo image{width:100%;height:100%;}
.userInfo{margin-left:40rpx;width:400rpx;height:120rpx;padding-top:100rpx;font-size:30rpx;color:#fff;}
.userInfo .userhead{display:inline-block;width:140rpx;height:140rpx;border-radius:140rpx;background:#fff;vertical-align:middle;}
.broadcast{position:absolute;left:30rpx;bottom:-80rpx;width:640rpx;height:100rpx;padding:30rpx;border-radius:10rpx;box-shadow:0 0 20rpx #999;background:#fff;z-index:10;vertical-align:middle;}
.dt{display:inline-block;width:120rpx;border-right:#dcdcdc 1rpx solid;text-align:center;}
.dt .day{display:block;font-size:60rpx;font-weight:700;line-height:1.1;}
.dt .month{display:block;font-size:32rpx;color:#999;line-height:1.1;}
.dd{display:inline-block;width:500rpx}
.ad{display:block;margin:120rpx 30rpx 30rpx;width:690rpx;height:156rpx;overflow:hidden;}
.ad image{width:100%;height:100%;}
.adlist{position:absolute;top:55rpx;line-height:30rpx;width:70%;font-size:30rpx;overflow:hidden;text-overflow:ellipsis;white-space: nowrap;}
.adlist em{display:inline-block;font-size:60rpx;padding:0 10rpx;color:#353b50;}
.listCox{margin:30rpx 40rpx;margin-top:100rpx;border-radius:10rpx;overflow:hidden;}
.icoMine{width:40rpx;height:40rpx;margin-right:15rpx;}

.broadcast .jtbtn image{width:40rpx;height:40rpx;}
.broadcast .jtbtn{position:absolute;top:60rpx;right:20rpx;width:40rpx;height:40rpx;}

