<view class='banner'>
  <image class="dianApplicateBanner" wx:if='{{preffixUrl}}' src="{{preffixUrl}}/static/wechat/img/apli_tou1.jpg"></image>
</view>

<view class="page">
  <image class="sucICO" wx:if='{{preffixUrl}}' src="{{preffixUrl}}/static/wechat/img/temp_suc.png"></image>
  <text class="tips">您的申请已受理</text>
  <view style="color:#0066b3;">
    订单编号：{{orderID}}
    <text style='color:#fff;font-size:22rpx;padding:6rpx 8rpx;background:#0066b3;' bindtap='copyText' data-text="{{orderID}}">复制</text>
  </view>
</view>
<view style='width:80%;margin:0 auto;'>
  <view class="weui-cell weui-cell_input">
      <view class="weui-cell__bd">
        <button class="button-hover" style="width:50%;margin:30rpx auto;" type='perimery' bindtap='toDetail'>进度查询</button>
      </view>
    </view>
</view>