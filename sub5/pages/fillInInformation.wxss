}
.text1{
  font-size: 26rpx;
  font-family: SourceHanSansSC-Regular;
  position:relative;
}
.text1:before{
  position:absolute;
  content:'*';
  left:-15rpx;
  top:-2rpx;
  color:#ff4444;
  font-size: 28rpx;
}
.text1>text{
  color: #2C3750;
  opacity: 0.7;
}
.switchrow{
  position: absolute;
  top: 20rpx;
