/* by .scss */
.container .bg {
  position: fixed;
  top: 0;
  left: 0;
  z-index: -1;
  width: 100%;
  height: 100%;
}

.form_submit, .form_upload {
  width: 690rpx;
  border-radius: 20rpx;
  background-image: linear-gradient(180deg, #FFFFFF 0%, #FFFFFF 100%);
  box-shadow: 0 2rpx 24rpx 0 #b3cbff;
  box-sizing: border-box;
  padding: 30rpx;
}

.form_submit .label_icon, .form_upload .label_icon {
  display: flex;
  align-items: center;
}

.form_submit .label_icon image, .form_upload .label_icon image {
  margin-left: 6rpx;
  width: 24rpx;
  height: 24rpx;
}

.form_submit .right_icon, .form_upload .right_icon {
  border-left: 1rpx solid #E3E7F0;
  padding-left: 16rpx;
}

.form_submit .right_icon image, .form_upload .right_icon image {
  width: 38rpx;
  height: 28rpx;
}

.btn_submit {
  background: #415FCD;
  box-shadow: 0 4rpx 8rpx 0 #8094de;
  border-radius: 42rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #FFFFFF;
  font-size: 30rpx;
  width: 450rpx;
  height: 80rpx;
}

.btn_submit[disabled] {
  background: #a0afe6 !important;
  color: #FFFFFF !important;
}

.form_upload {
  margin: 50rpx auto;
  font-size: 24rpx;
  color: #7C8BAC;
}

.form_upload > view:nth-of-type(1) {
  display: flex;
  align-items: center;
  font-size: 30rpx;
  color: #2C3750;
}

.form_upload > view:nth-of-type(1) image {
  width: 10rpx;
  height: 47rpx;
  margin-left: -30rpx;
  margin-right: 30rpx;
}

.form_upload > view:nth-of-type(3) {
  background: #F7FBFF;
  border: 1rpx solid #D2DDFF;
  border-radius: 10rpx;
  padding: 20rpx;
  margin-top: 40rpx;
  line-height: 38rpx;
}

.form_upload > view:nth-of-type(3) image {
  width: 26rpx;
  height: 26rpx;
  margin-right: 8rpx;
}

.form_upload > view:nth-of-type(3) > view:nth-of-type(1) {
  display: flex;
  align-items: center;
}

.form_upload > view:nth-of-type(3) text {
  color: #415FCD;
}

.form_dialog {
  background: none !important;
}

.form_dialog .form_upload {
  margin: 20rpx auto;
}

.agreement {
  padding: 20rpx;
  font-size: 30rpx;
  color: #2C3750;
}

.agreement > view:nth-of-type(1) {
  font-size: 34rpx;
}

.agreement > view:nth-of-type(2) {
  margin: 40rpx auto;
}

.agreement > view:nth-of-type(2) > view {
  display: flex;
  align-items: center;
  padding: 20rpx 50rpx;
}

.agreement > view:nth-of-type(2) > view image {
  width: 74rpx;
  height: 78rpx;
  margin-right: 10rpx;
}

.agreement .btn_submit {
  width: 450rpx;
}

.agreement .btn_submit + view {
  width: 380rpx;
  margin: 30rpx auto;
}

.agreement .ckb {
  font-size: 26rpx;
  color: #666666;
}

.agreement {
  background: #F2F7FF;
  width: 100%;
  overflow-x: hidden;
}

.agreement .pdf {
  width: 690rpx;
  margin: 0 auto;
  height: 1263rpx;
  overflow-x: hidden;
  padding-bottom: 150rpx;
}

.agreement .pdf image {
  width: 100%;
  height: 100%;
}

.agreement .pdf .title {
  font-size: 50rpx;
  font-weight: 700;
  text-align: center;
  letter-spacing: 16rpx;
}

.agreement .pdf .ts {
  font-size: 40rpx;
  font-weight: 700;
  text-align: center;
  letter-spacing: 6rpx;
}

.agreement .pdf .jsyh {
  font-size: 32rpx;
  letter-spacing: 3rpx;
  margin-bottom: 10rpx;
}

.agreement .pdf .contont {
  font-size: 32rpx;
  text-indent: 64rpx;
  line-height: 56rpx;
  margin-bottom: 20rpx;
}

.agreement .pdf .userInfo {
  width: 100%;
  overflow: hidden;
}

.agreement .pdf .userContont {
  width: 575rpx;
  float: right;
  font-size: 29rpx;
  margin-bottom: 20rpx;
  letter-spacing: 6rpx;
}

.agreement .pdf .bottomtext {
  margin-top: 20rpx;
  font-weight: 600;
}

.agreement .pdf .zhong {
  font-weight: 600;
  text-decoration: underline;
}

.agreement .pdf .titletwe {
  font-size: 36rpx;
  font-weight: 700;
  text-align: center;
  letter-spacing: 8rpx;
}

.agreement .pdf .htmb {
  width: 100%;
  height: 100%;
  position: relative;
}

.agreement .pdf .custName {
  position: absolute;
  top: 145rpx;
  left: 170rpx;
  font-size: 24rpx;
  z-index: 100;
}

.agreement .pdf .creditCode {
  position: absolute;
  top: 220rpx;
  left: 210rpx;
  font-size: 24rpx;
  z-index: 100;
}

.agreement .pdf .custName2 {
  position: absolute;
  top: 810rpx;
  right: 50rpx;
  font-size: 24rpx;
  z-index: 100;
}

.agreement .pdf .year {
  position: absolute;
  top: 840rpx;
  right: 50rpx;
  font-size: 24rpx;
  z-index: 100;
}

.agreement .pdf .month {
  position: relative;
  top: -489rpx;
  left: 500rpx;
  font-size: 24rpx;
  z-index: 100;
}

.agreement .pdf .day {
  position: relative;
  top: -519rpx;
  left: 557rpx;
  font-size: 24rpx;
  z-index: 100;
}

.agreement .pdf .bottomtext {
  margin-top: 20rpx;
  font-weight: 600;
}

.agreement .pdf .fontweight {
  font-weight: bold;
}

.bottom {
  background: rgba(255, 255, 255, 0.94);
  box-shadow: 0 0 27rpx 0 rgba(7, 102, 255, 0.07);
  width: 100%;
  position: fixed;
  bottom: 0;
}

.bottom .btn_submit {
  width: 690rpx;
  margin: 20rpx auto;
}
