.table {
  width:100%;
  display: flex;
  flex-direction: column;     /* 排列形式: 向下 */
}

.tr {
  display: flex;
  flex-direction: row;
}
.table>.tr:nth-of-type(1){
  background: #EBF0FF;
  border-radius: 16rpx 16rpx 0 0;
}
.th, .td {  /* 公有的属性 */
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;            /* 自动换行 */
  width: 50%;					/* 4个25%相加刚好100% */
  text-align: center;         /* 文本居中 */
  justify-content:center;     /* 主轴居中 */
  align-items: center;        /* 交叉轴居中 */
}

.th {
