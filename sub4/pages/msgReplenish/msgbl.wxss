page,view,text,swiper,swiper-item,image,navigator,textarea{
  padding:0;
  margin:0;
  box-sizing:border-box;
}
.container_box{
  width:100%;
  position:relative;
}
.container_box_bg{
  width:100%;
  height:470rpx;
  position:absolute;
  top:0;
  left:0;
  z-index:-1;
}
.container_box_logo{
  width:280rpx;
  height:73rpx;
  z-index:-1;
  margin-top:55rpx;
  margin-left:30rpx;
}
.top1{
  font-size: 42rpx;
  color: #FFFFFF;
  margin-left:32rpx;
  margin-top:26rpx;
}
.top2{
  width:100%;
  padding:28rpx 0 70rpx 0;
  text-align:center;
  font-size:26rpx;
  color:#fff;
  background: rgba(0,0,0,0.19);
  margin-top:29rpx;
}
.contentbox{
  width:100%;
  background: #F0F2F5;
  border-radius: 20rpx 20rpx 8rpx 8rpx;
  margin-top:-40rpx;
  padding-bottom: 80rpx;
}
.box1{
  width:100%;
  background:#fff;
  padding:40rpx 30rpx 20rpx 30rpx;
  border-radius: 20rpx 20rpx 8rpx 8rpx;
}
.box2{
  width:100%;
  background:#fff;
  padding:40rpx 30rpx 20rpx 30rpx;
  position:relative;
}
.viewbox{
  position:absolute;
  bottom:0;
  left:0;
  width:100%;
  background:transparent;
  z-index:3;
}
.title{
  font-size:34rpx;
  font-weight:bold;
  display:flex;
  align-items: center;
  margin-bottom:10rpx;
}
.title_img{
  width:8rpx;
  height:30rpx; 
  margin-right:12rpx;
}
.row1{
  padding:30rpx 0;
  border-bottom: 1px solid #E9E9E9;
  display: flex;
  justify-content:space-between;
  font-size: 30rpx;
  color: #3E415C;
}
.box1 .row1:last-child,
.box2 .row1:last-child{
  border:none;
}
.inputstyle{
  text-align:right;
  z-index:0;
}
.placeholderStyle{
  color: #CCCCCC;
}
.logocenter{
position:relative;
width:100%;
height:70rpx;
}
.logo_small{
  position:absolute;
  right:30rpx;
  width:158rpx;
  height:41rpx;
  margin:15rpx 0;
}
.title1{
  font-size: 30rpx;
  color: #3E415C;
}
.textareaStyle{
  width:100%;
  background: #F8FCFF;
  border-radius: 8rpx;
  height:120rpx;
  overflow: auto;
  padding:20rpx;
  margin-top:20rpx;
}
.btn_blue_tj{
  width:451rpx;
  height:84rpx;
  line-height:84rpx;
  margin:80rpx auto;
  margin-bottom: 0;
  background: #1677FF;
  box-shadow: 0 3rpx 11rpx 0 rgba(115,149,255,0.64);
  border-radius: 42rpx;
  color:#fff;
  text-align:center;
}
.greybox{
  width:100%;
  height:100vh;
  background:rgba(0,0,0,0.5);
  position: fixed;
  top:0;
  z-index:2;
}
.bottomchoose{
  width: 100%;
  background: #f7f7f7;
  position: fixed;
  bottom: 0;
  border-radius: 20rpx 20rpx 0 0;
  z-index:1;
}
.bottomchoose .row2:first-of-type{
  border-radius: 20rpx 20rpx 0 0;
}
.row2{
  width:100%;
  height:100rpx;
  line-height:100rpx;
  text-align: center;
  border-bottom: 1px solid #E9E9E9;
  background:#fff;
  font-size: 30rpx;
}
.personsyr{
  margin-top: 30rpx;
  font-weight: bold;
}