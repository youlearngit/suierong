page {
	background: #F5F7FF;
}
.plane {
	padding: 30rpx;
}
.bg {
	position: fixed;
	width: 100%;
    top: 0;
    left: 0;
    z-index: -1;
}
.head {
	padding: 20rpx 0;
	display: flex;
	align-items: center;
}
.head image {
	margin-right: 15rpx;
	width: 33rpx;
	height: 44rpx;
}
.head-search {
	line-height: 50rpx;
	font-size: 34rpx;
	color: #3E415C;
}
.head-tip {
	margin-left: 8rpx;
	font-size: 26rpx;
	color: rgba(62,65,92,0.46);
}
.search {
	position: relative;
	padding: 20rpx 0 44rpx 0;
}
.search input {
	width: 100%;
	height: 76rpx;
	padding: 0px 73rpx;
	background: #ffffff73;
	border-radius: 40rpx;
	font-size: 28rpx;
	box-sizing: border-box;
}
.search-placeholder {
	color: #C5C5C5;
}
.search-icon {
	position: absolute;
	width: 32rpx;
	height: 32rpx;
	top: 45rpx;
	left: 32rpx;
}
.search-btn {
	z-index: 2;
	width: 110rpx;
	height: 63rpx;
	position: absolute;
	top: 27rpx;
	right: 8rpx;
	/* background-image: linear-gradient(130deg, #A4AFFD 0%, #6B77F9 100%); */
	border-radius: 30rpx;
	display: flex;
    align-items: center;
    justify-content: center;
}
.search-btn image {
	width: 60rpx;
	height: 42rpx;
}
.keyword {
	background: yellow;
}
.recommend {
	text-align: center;
	padding-bottom: 59rpx;
}
.recommend image {
	width: 318rpx;
	height: 225rpx;
}
.recommend view {
	font-size: 26rpx;
	color: #98A3CB;
	line-height: 40rpx;
}
.go-filter {
	display: flex;
	align-items: center;
	font-size: 24rpx;
	color: #259f0b;
}
.go-filter image {
	margin-right: 10rpx;
	width: 32rpx;
	height: 32rpx;
}
.article {
	position: relative;
	padding: 21rpx 40rpx 27rpx 40rpx;
	width: 690rpx;
	/* height: 246rpx; */
	box-sizing: border-box;
	margin: 20rpx 0;
	background: #FFFFFF;
	border-radius: 12rpx;
	box-shadow: 0 2rpx 12rpx 0 rgba(126,152,195,0.15);
}
.article-loc {
	position: absolute;
	z-index: 0;
	top: 0;
	right: 0;
	width: 130rpx;
	color: #FFFFFF;
	font-size: 26rpx;
	text-align: center;
}
.article-loc image {
	position: absolute;
	z-index: -1;
	width: 130rpx;
	height: 38rpx;
	top: 0;
	left: 0;
}
.article-title {
	line-height: 45rpx;
	max-width: 500rpx;
	font-size: 30rpx;
	color: #3E415C;
	/* white-space: nowrap; */
    overflow: hidden;
    text-overflow: ellipsis;
}
.article-subtitle {
	font-size: 26rpx;
	color: rgba(28,41,79,0.6);
}
.article-details {
	margin: 27rpx 0 18rpx 0;
	background: rgba(247,249,255,0.98);
	border: 1rpx solid rgba(235,240,255,0.98);
	border-radius: 8rpx;
	display: flex;
    align-items: center;
    justify-content: space-evenly;
}
.article-splitline {
	width: 1rpx;
	height: 35rpx;
	background-color: rgba(62,65,92,0.3);
}
.article-detail {
	padding: 12rpx 0 9rpx 0;
	text-align: center;
}
.article-detail view:nth-child(1) {
	font-size: 28rpx;
	color: #3E415C;
}
.article-detail view:nth-child(2) {
	font-size: 24rpx;
	color: rgba(28,41,79,0.6);
}
.filter-body {
	font-size: 26rpx;
}
.filter-head {
	margin: 0 30rpx;
	padding: 30rpx 0;
	display: flex;
	align-items: center;
	justify-content: space-between;
	font-size: 34rpx;
	color: #333333;
	border-bottom: 1rpx solid #E7E7EF;
}
.filter-reset {
	font-size: 26rpx;
	color: #446DFF;
}
.filter-title {
	padding: 30rpx 0;
	display: flex;
	align-items: center;
	font-size: 30rpx;
	color: #3E415C;
}
.filter-title-icon {
	margin-right: 24rpx;
	width: 6rpx;
	height: 30rpx;
	background: #446DFF;
	border-radius: 3rpx;
}
.filter-confirm {
	margin: 80rpx auto;
	width: 444rpx;
	height: 86rpx;
	display: flex;
	align-items: center;
	justify-content: center;
	background: #446DFF;
	border-radius: 43rpx;
	font-size: 34rpx;
	color: #FFFFFF;
}
.filter-opt {
	padding: 12rpx 30rpx;
	display: flex;
	align-items: center;
	color: #3E415C;
}
.filter-opt-text {
	margin-right: 20rpx;
}
.filter-checkbox {
	padding: 12rpx;
	overflow-y: hidden;
	overflow-x: scroll;
}
.filter-checkbox::-webkit-scrollbar {
	display: none;
	width: 0;
	height: 0;
	color: transparent;
}
.filter-checkbox view {
	position: relative;
	margin: 0 20rpx;
	padding: 15rpx 47rpx;
	background: #F5F6FC;
	border-radius: 10rpx;
	white-space: nowrap;
	font-size: 26rpx;
	color: #444A6B;
}
.filter-checkbox image {
	position: absolute;
	top: -12rpx;
	right: -12rpx;
	width: 24rpx;
	height: 24rpx;
}
.filter-checkbox .activity {
	border: 2rpx solid #446DFF;
	color: #446DFF;
}
.filter-daterange {
	white-space: nowrap;
}
.filter-daterange-date {
	position: relative;
	display: flex;
	align-items: center;
}
.filter-daterange-date input {
	padding: 0 20rpx;
	width: 263rpx;
	height: 70rpx;
	border: 1rpx solid rgba(126,133,155,0.5);
	border-radius: 10rpx;
	box-sizing: border-box;
	font-size: 28rpx;
	color: #333333;
}
.filter-daterange-date .input-placeholder {
	color: #A2A5BA;
}
.filter-picker-multi input {
	padding: 0 20rpx;
	width: 400rpx;
	height: 70rpx;
	border: 1rpx solid rgba(126,133,155,0.5);
	border-radius: 10rpx;
	box-sizing: border-box;
	font-size: 28rpx;
	color: #333333;
}
.filter-daterange-splitline {
	margin: 0 10rpx;
	width: 23rpx;
	height: 1rpx;
	background-color: #D2DAF2;
}
.filter-daterange-date image {
	position: absolute;
	right: 20rpx;
	width: 38rpx;
	height: 38rpx;
}
