/* sub3/pages/more/index.wxss *//* pages/component/nav/nav.wxss */
@import "/assets/styles/weui.wxss";
@import "/assets/styles/colorui/main.wxss";
@import "/assets/styles/colorui/animation.wxss";
@import "/assets/styles/colorui/icon.wxss";

page {
	background-color: white;
}

.nav-header {
	width: 100%;
	position: absolute;
	left: 0;
	top: 0;
}

.VerticalNav.nav {
	width: 260rpx;
	white-space: initial;
	z-index: 99;
	background-color: #f6faff;
}

.VerticalNav.nav .cu-item {
	width: 100%;
	text-align: center;
	background-color: #fff;
	margin: 0;
	border: none;
	height: 112rpx;
	line-height: 112rpx;
	position: relative;
}

.VerticalNav.nav .cu-item.cur {
	background-color: #f1f1f1;
	font-family: PingFangSC-Medium;
	font-size: 30rpx;
	color: #333333;
}

.VerticalNav.nav .cu-item.cur::after {
	content: "";
	width: 10rpx;
	height: 31rpx;
	position: absolute;
	background-color: currentColor;
	top: 0;
	right: 0rpx;
	bottom: 0;
	margin: auto;
}

.VerticalBox {
	display: flex;
	padding: 130rpx 0;
}

.text-green,
.line-green,
.lines-green {
	color: #353b50;
}

.productBImg {
	width: 100%;
}

.cu-item image {
	margin: 0 0 20rpx;
}

.cu-list.menu-avatar > .cu-item .content {
	position: absolute;
	left: 30rpx;
	width: 95%;
	top: 46rpx;
	display: flex;
}

.cu-list.menu-avatar {
	padding-left: 28rpx;
	display: flex;
	flex-flow: row wrap;
}
.cu-list.menu-avatar > .cu-item .content2 {
	position: absolute;
	left: 30rpx;
	width: 95%;
	top: 46rpx;
}

.producticon {
	width: 80rpx;
	height: 80rpx;
}

.producticonImg {
	width: 80rpx;
	height: 80rpx;
}

.producticonBox {
	display: flex;

	width: 100%;
	flex-flow: row wrap;
}

.zh-product {
	width: 100%;
	display: flex;
	flex-flow: row wrap;
}

.producticonBox2 {
	height: 150rpx;
	width: 33.3%;
	text-align: center;
}

.title2 {
	font-family: PingFangSC-Regular;
	font-size: 25.74rpx;
	color: #4a4a4a;
	text-align: center;
}

.producticonImg2 {
	width: 80rpx;
	height: 80rpx;
}

.producticonImg3 {
	width: 60rpx;
}

.productdesc {
	margin-left: 20rpx;
}

.title {
	font-family: PingFangSC-Semibold;
	font-size: 30rpx;
	color: #4a4a4a;
	line-height: 36rpx;
	margin-bottom: 10rpx;
	margin-top: -5rpx;
}

.desc {
	font-family: PingFangSC-Regular;
	font-size: 24rpx;
	color: #999999;
	line-height: 24rpx;
}

.cu-list.menu-avatar > .cu-item {
	margin: 21rpx;
	position: relative;
	width: 180rpx;
    height: 180rpx;
	display: flex;
	align-items: center;
	justify-content: center;
	background: #f6faff;
	border-radius: 8rpx;
}

.VerticalNav.nav .cu-item.cur::after {
	content: "";
	width: 0rpx;
}

.VerticalNav.nav .cu-item.cur::before {
	content: "";
	width: 10rpx;
	height: 43rpx;
	border-radius: 0 27rpx 36rpx 0;
	position: absolute;
	background-color: #4586e6;
	top: 0;
	left: 0rpx;
	bottom: 0;
	margin: auto;
}

.VerticalNav.nav .cu-item.cur {
	background-color: #fff;
	font-size: 32rpx;
	font-family: PingFangSC-Medium;
	color: #151515;
}

.VerticalMain {
	background-color: white;
	z-index: 9999;
	padding: 40rpx 0;
}

.VerticalNav.nav .cu-item {
	background-color: #f6faff;
	font-size: 26rpx;
	font-family: PingFangSC-Regular;
	color: #333333;
}

.cu-item-name {
	position: absolute;
	top: 126rpx;
	font-size: 24rpx;
	color: #4A4A4A;
}

.tip{
	z-index: -2;
	position: fixed;
	bottom: 121rpx;
	right: 74rpx;
}
.tipTxt{
	margin-top: 17rpx;
	font-family: SourceHanSansSC-Regular;
	font-size: 26rpx;
	color: #ADCCFF;
	width: 100%;
	text-align: center;
}