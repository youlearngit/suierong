.home_bg {
	width: 100%;
	position: fixed;
    top: 0;
    left: 0;
    z-index: -1;
}
.home_btns {
	position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    padding: 95rpx;
    text-align: center;
    box-sizing: border-box;
}
.home_btn {
	width: 560rpx;
	height: 100rpx;
	margin: 40rpx 0;
	border-radius: 50rpx;
	line-height: 100rpx;
	font-family: SourceHanSansCN-Medium;
	font-size: 34rpx;
	letter-spacing: 0;
	text-align: center;
	font-weight: 500;
	box-sizing: border-box;
}
.home_btn_apply {
	background-image: linear-gradient(270deg, #7C88FF 0%, #266CFF 100%);
	box-shadow: 0 4rpx 16rpx 0 #879EFF;
	color: #FFFFFF;
}
.home_btn_mine {
	border: #3E74FF solid 1rpx;
	color: #3E74FF;
}