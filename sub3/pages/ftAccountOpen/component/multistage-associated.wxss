}
.acc_typesItem5 {
  padding: 11rpx 14rpx;
  font-size: 26rpx;
  font-family: SourceHanSansCN-Regular;
  color: #1c294f;
  letter-spacing: 0;
}
.acc_typesItem5Select {
  padding: 11rpx 14rpx;
  font-family: SourceHanSansCN-Regular;
  font-size: 26rpx;
  color: #ffffff;
