.line-split{
  border: 1rpx solid #E5E6EA;
  width: 650rpx;
  margin-left: 20rpx;
}
label{
  font-family: SourceHanSansSC-Regular;
  font-size: 28rpx;
  color: #7C8BA4;
  letter-spacing: 0;
}
.info-text{
  font-family: SourceHanSansSC-Regular;
  font-size: 28rpx;
  color: #333333;
  letter-spacing: 0;
  float: right;
}
/* 按钮 */
.foot{
  width: 690rpx;
  height: 120rpx;
  margin-top: 80rpx;
}
button{
  border-radius: 40rpx;
  font-size: 30rpx;
}
.btn-cancel{
  width: 229rpx;
  height: 80rpx;
  float: left;
  border: 1rpx solid #3D5FE9;
  color: #3D5FE9;
}
.btn-submit{
  width: 442rpx;
  height: 80rpx;
  float: right;
  background:linear-gradient(to bottom, #8BA2FD,#3D5FE9);
  color: #FFFFFF;
}
.contaioner-top{
  height: 8%;
  background-color: #ffffff;
  line-height: 120rpx;
}
.contaioner-top::before{
  content: '';
  position: absolute;
  top: 110rpx;
  left: 25rpx;
  bottom: auto;
  right: auto;
  height: 1rpx;
  width: 700rpx;
  background-color: #E5E6EA;
}
.top-left{
  font-family: SourceHanSansCN-Medium;
  font-size: 34rpx;
  color: #1C294F;
  text-align: center;
}
.top-right{
  position: relative;
  top: -120rpx;
  left: 660rpx;
  width: 60rpx;
  font-family: SourceHanSansSC-Regular;
  font-size: 28rpx;
  color: #6580ED;
  text-align: right;
}
.contaioner-content{
  margin: 20rpx 25rpx;
  height: 81.2%;
  width: 95%;
  overflow: scroll;
}
.contaioner-foot{
  height: 154rpx;
  width: 750rpx;
  position: fixed;
  background-color: #ffffff;
  box-shadow:0px 0px 80rpx 10rpx #e1ebff;
  top: 90%;
  padding: 35rpx 50rpx 29rpx 50rpx;
}
.cont-btn-submit{
  margin-bottom: 0rpx;
  width: 80%;
  height: 80rpx;
  padding-top: 10rpx;
  font-family: SourceHanSansSC-Regular;
  font-size: 30rpx;
  color: #FFFFFF;
  text-align: center;
  background:linear-gradient(to bottom, #8BA2FD,#3D5FE9);
  box-shadow: 0 4px 12px 0 rgba(110,137,245,0.33);
  border-radius: 40rpx;
}

.page-height{
  padding: 0rpx 0rpx;
}
.version{
  margin-right: 20rpx;
  font-family:'Courier New', Courier, monospace;
  float: right;
}
.title-img{
  width: 700rpx;
  height: 150rpx;
  margin-top: 20rpx;
}
.title-big-strong{
  font-family: 'SimHei';
