page {
  background-color: white;
  overflow: hidden;
  background-image: linear-gradient(178deg, #95BAFF 0%, #FFFFFF 95%);
}
.contract-background-img{
  width: 100%;
  height: 100%;
  background-size: 120%;
  overflow: auto;
  padding: 50rpx 32rpx 20rpx 32rpx;
}
.box{ 
  width: 690rpx;
  height: 1117rpx;
  background-image: linear-gradient(180deg, #FFFFFF 0%, #FFFFFF 100%);
  box-shadow: 0 2rpx 24rpx 0 rgba(156,187,255,0.56);
  border-radius: 18rpx;
}
.info{
  height: 120rpx;
  line-height: 120rpx;
  margin: 0rpx 30rpx;
  padding: 0rpx 15rpx;
}
.info2{
  height: 124rpx;
  line-height: 124rpx;
  margin: 0rpx 45rpx;
}
