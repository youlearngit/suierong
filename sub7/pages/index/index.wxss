page,view,text,swiper,swiper-item,image,navigator{
  padding:0;
  margin:0;
  box-sizing:border-box;
}
.container_content{
  width:100%;
  height:1410rpx;
  position: relative;
  padding-top:120rpx;
}
.container_bg{
  position:absolute;
  width:100%;
  height:100%;
  top:0;
  left:0;
  z-index:-1;
}
.title{
  font-size: 38rpx;
  color: #1C2652;
  margin-left:60rpx;
  font-weight:500;
}
.box{
  padding:0 30rpx;
  /* margin-top:110rpx; */
  display:flex;
  overflow: hidden;
  flex-wrap: wrap;
  justify-content: space-between;
}
.box>view{
  width:335rpx;
  height:202rpx;
  position:relative;
  padding:30rpx;
  margin-bottom:26rpx;
}
.boxImg{
  position:absolute;
  z-index:-1;
  top:0;
  left:0;
  width:100%;
  height:100%;
}
.box>view>view:nth-of-type(1){
  font-size: 24rpx;
  color: #7881A8;
  margin-bottom:40rpx;
}
.box>view>view:nth-of-type(2){
  font-size: 50rpx;
  color: #1C2652;
}
.text1{
  font-size: 28rpx;
  color: #1C2652;
  margin-left:30rpx;
  margin-bottom:50rpx;
}
.text1>text{
  font-size: 22rpx;
  color: #7881A8;
  margin-left:16rpx;
}



.bottom_content{
    position: fixed;
    height:104rpx;
    bottom: 0;
    width: 100%;
    background: #fff;
    z-index: 99;
    display: flex;
    justify-content: space-around;
    font-size: 22rpx;
    color: #C3C4D0;
    padding-top:10rpx;
}
.bottom_flex{
    text-align:center;
}
._fleximg{
    width:56rpx;
    height:56rpx;
    display:block;
    margin:0 auto;
}
.bottom_flex.active{
    color: #168BFB;
}