.diya{font-size:26rpx;color:#ccc;}
.loginWrap{position:fixed;top:0;left:0;width:100%;height:100%;background:rgba(0,0,0,0.3);z-index:1000;}
.login{position:fixed;top:30%;left:12.5%;width:75%;height:40%;background:#fff;box-shadow:0 0 8rpx #ccc;border-radius:10rpx;}
.login .button{position:absolute;bottom:20rpx;width:35%;height:60rpx;border-radius:60rpx;font-size:30rpx;text-align:center;line-height:60rpx;color:#fff;}
.login .button.cancel{left:10%;background:#ccc;}
.login .button.do{right:10%;background:#0066b3;}
.login .tips{margin-top:7%;text-align:center;}
.login .tips .h2{font-size:36rpx;color:#333;margin-bottom:10rpx;}
.login .tips .p{font-size:24rpx;color:#888;}
.login .slogan{position:absolute;top:35%;left:0;width:100%;height:200rpx;text-align:center;font-size:26rpx;color: #0267f1;}
.login .slogan image{display:inline-block;width:120rpx;height:123rpx;margin-bottom:20rpx;}
.beback{position:absolute;width:80%;height:26rpx;line-height:26rpx;font-size:26rpx;font-weight:400;color:#333;top:30rpx;left:30rpx;z-index:10;}
.beback em{display:inline-block;width:18rpx;height:18rpx;border-bottom:2rpx solid #333;border-left:2rpx solid #333;transform:rotate(45deg);vertical-align:middle}
.beback text{vertical-align:middle}


