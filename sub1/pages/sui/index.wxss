}

.beback {
  position: absolute;
  width: 80%;
  height: 26rpx;
  line-height: 26rpx;
  font-size: 26rpx;
  font-weight: 400;
  color: #333;
  top: 30rpx;
  left: 30rpx;
  z-index: 10;
}

.beback em {
  display: inline-block;
  width: 18rpx;
