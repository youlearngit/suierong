    <block wx:if="{{productId == 2}}">
        <image class="f1" wx:if='{{preffixUrl}}' src="{{preffixUrl}}/static/wechat/img/zm/zm_24.png" mode="widthFix" lazy-load="false" binderror="" bindload=""></image>
