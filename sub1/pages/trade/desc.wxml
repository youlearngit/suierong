    <block wx:if="{{productId == 1}}">
        <image class="f1" wx:if='{{preffixUrl}}' src="{{preffixUrl}}/static/wechat/img/zm/zm_22.png" mode="widthFix" lazy-load="false" binderror="" bindload=""></image>
