                </view>
                <view class="fr">
                    <view class="p">授权人签字：{{customerInfo.NAME}}</view>
                    <view class="p">身份证号：{{customerInfo.ID_NUMBER}}</view>
                    <view class="p">日期：{{day_time}}</view>
                </view>
            </view>
