.water_top{
  position: fixed;
  top:100rpx;
  bottom: 0;
  left: 20rpx;
  right: 0;
  transform:rotate(-25deg);
  opacity: 0.1;
  z-index: 1;
}
.water-text{
  float: left;
  width:375rpx;
  color: rgba(169,169,169,.2);
  text-align: center;
  font-size: 40rpx;
  margin: 100rpx 0;
}
.watermark {
  position: fixed;
  top: 0;
  width: 100%;
  height: 100%;
  background: #eeeeee11;
  pointer-events: none;
  background-repeat: repeat;
  background-size: 50% auto;
}
.water-text1{
  width:250rpx;
  height:70rpx;
  float: left;
  color: rgba(169,169,169,.2);
  margin: 100rpx 0;
  margin-right: 100rpx;
}